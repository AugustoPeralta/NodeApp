{
  "name": "augustodemo",
  "version": "0.0.0-development",
  "main": "index.js",
  "devDependencies": {
    "jest": "^25.1.0",
    "semantic-release-ado": "^1.2.2"
  },
  "scripts": {
    "test": "jest test.js"
  },
  
  "release": {
    "branches": [
      "master"
    ],
    "plugins": [
